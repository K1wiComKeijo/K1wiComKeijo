local Players = game:service("Players")
local Player = Players.LocalPlayer
local userInput = game:service("UserInputService")
local runService = game:service("RunService")
repeat wait() until Player.Character
local Character = Player.Character
local pHum = Character:WaitForChild("Humanoid")
local humRoot = Character:WaitForChild("HumanoidRootPart")
local UserInputService = game:GetService("UserInputService")
local GuiService = game:GetService("GuiService")
local LocalPlayer = game:GetService("Players").LocalPlayer
local Multiplier = 0
local intens = 0
local VSPEED = 40
local GV = 196.1999969482422
local a=game.Players.LocalPlayer
local b=a.Character
local c=b:WaitForChild("Humanoid")

for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.Idled)) do
   v:Disable()
end

game:GetService("RunService").Stepped:Connect(function()
    if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
        local Humanoid = workspace.CurrentCamera.CameraSubject;
        if Humanoid:IsA("Humanoid") then
            Humanoid.SeatPart:ApplyImpulse(Humanoid.SeatPart.CFrame.LookVector * Vector3.new(intens, intens, intens))
        elseif Humanoid:IsA("BasePart") then
            Humanoid:ApplyImpulse(Humanoid.CFrame.LookVector * Vector3.new(intens, intens, intens))
        end
    end
 end)

local player = game.Players.LocalPlayer.Character

if game.Players.LocalPlayer.Character == nil then
   print("Couldn't get player name")
end

print(player, "- Got player's name..")

task.wait(Random.new():NextNumber(0.05, 0.15))

print("Loading gui..")

local uilibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kiet1308/tvkhub/main/rac"))()
local windowz = uilibrary:CreateWindow("Made by voxels86", "Oaklands [2023-2024 public]", true)

local Main = windowz:CreatePage("Main")


local Section1Main = Main:CreateSection("Main")

Section1Main:CreateButton("Goto Meteor", function ()

    local player = game.Players.LocalPlayer.Character
    local part = game:GetService("Workspace").World.Effects.Smoke.Part
    player.HumanoidRootPart.CFrame = part.CFrame

    wait (1)

    for _,p in pairs(game.Players:GetPlayers()) do

        p.Character:WaitForChild("Humanoid").Jump = true
        
        end

    local player = game.Players.LocalPlayer.Character
    local part = game:GetService("Workspace").World.Effects.Crater.Torus
    player.HumanoidRootPart.CFrame = part.CFrame

    for i,v in pairs(workspace:GetDescendants()) do
        if v:IsA("Model") and v.Name == "Smoke" then
           v:Destroy()
        else
            
        end
    end

end)

Section1Main:CreateToggle("Anti Spore Attack", {Toggled=false , Description = false}, function(AS)
    shared["ANTISPORE"] = AS
    while shared["ANTISPORE"] do
      task.wait (0.01)
      if not shared["ANTISPORE"] then
          break
      end
    for i,v in pairs(workspace.World.Effects:GetDescendants()) do
        if v:IsA("Part") and v.Name == "Spore" then
            v:Destroy()
        end
    end
  end
 end)

 Section1Main:CreateToggle("Walk & Drive On Water + Lava + Acid -- PERMA", {Toggled=false , Description = false}, function(FW)
    shared["FLUIDWALK"] = FW
    while shared["FLUIDWALK"] do
      task.wait (0.01)
      if not shared["FLUIDWALK"] then
          break
      end
      wait(0.1)
      for i,v in pairs(workspace.World.Ocean.Water:GetDescendants()) do
        if v:IsA("Part") and v.Name == "Water" then
           v.CanCollide = true
        end
    end
    wait(0.1)
    for i,v in pairs(workspace.World.Regions.Gameplay.Damage.Acid:GetDescendants()) do
        if v:IsA("Part") and v.Name == "Acid" then
           v.CanCollide = true
        end
    end
    wait(0.1)
    for i,v in pairs(workspace.World.Regions.Gameplay.Damage.Lava:GetDescendants()) do
        if v:IsA("Part") and v.Name == "Lava" then
           v.CanCollide = true
        end
    end
  end
 end)

local Section2Main = Main:CreateSection("Player")

Section2Main:CreateSlider("Walkspeed Multiplier -- Press Shift/Sprint", {Min = 0, Max = 5, DefaultValue = 0}, function(MP)
    Multiplier = MP
end)

Section2Main:CreateTextbox("Vehicle CFrame -- Press L-CTRL (put above 5000)", false, function (RS)
     intens = RS
 end)

 Section2Main:CreateButton("Activate VSpeed (click when in a car)", function ()
    for i,v in pairs(workspace.World.Structures:GetDescendants()) do
        if v:IsA("VehicleSeat") and v.Occupant == c then
            e = v
        end
    end
end)

 Section2Main:CreateSlider("Vehicle Speed", {Min = 0, Max = 100, DefaultValue = 40}, function(VS)
    VSPEED = VS

    e.MaxSpeed = VSPEED
end)

Section2Main:CreateSlider("FOV Slider", {Min = 0, Max = 120, DefaultValue = 70}, function(FOV)
    workspace.CurrentCamera.FieldOfView = FOV
end)

runService.Stepped:connect(function()
    if userInput:IsKeyDown(Enum.KeyCode.LeftShift) then
    humRoot.CFrame = humRoot.CFrame + pHum.MoveDirection * Multiplier
    end
    end)

Section2Main:CreateButton("Infinite zoom -- Noclip zoom", function ()
   game.Players.LocalPlayer.DevCameraOcclusionMode = "Invisicam"
   game.Players.LocalPlayer.CameraMaxZoomDistance = 1000000000000000000000000000000000000000000000000
end)

Section2Main:CreateButton("Infinite jump", function ()
 local InfJump = true
 game:GetService("UserInputService").jumpRequest:Connect(function()
     if InfJump then
         game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass"Humanoid":ChangeState("Jumping")
     end
 end)
 end)

 local Section3Main = Main:CreateSection("Misc")

 Section3Main:CreateSlider("Gravity Slider", {Min = 0, Max = 196.1999969482422, DefaultValue = 196.1999969482422}, function(GN)
    GV = GN

    game.Workspace.Gravity = GV
end)

 Section3Main:CreateButton("Fullbright", function ()
   local Light = game:GetService("Lighting")
 
   function dofullbright()
   Light.Ambient = Color3.new(1, 1, 1)
   Light.ColorShift_Bottom = Color3.new(1, 1, 1)
   Light.ColorShift_Top = Color3.new(1, 1, 1)
   end
    
   dofullbright()
    
   Light.LightingChanged:Connect(dofullbright)
 end)

 Section3Main:CreateButton("No fog", function ()

   game.Lighting.Sky:Destroy()
   game.Lighting.Bloom:Destroy()
   game.Lighting.Folder:Destroy()
   game.Lighting.Blur:Destroy()
   game.Lighting.ColorCorrection:Destroy()
   game.Lighting.SunRays:Destroy()
   game.Lighting.WaterBlur:Destroy()
   game.Lighting.Atmosphere:Destroy()
   game.Lighting.DepthOfField:Destroy()

end)

Section3Main:CreateButton("No NetPause Notification", function ()
    game.CoreGui.RobloxNetworkPauseNotification:Destroy()
end)

Section3Main:CreateButton("No WaterEffect", function ()

    game.Players.LocalPlayer.PlayerGui.WaterEffect:Destroy()

 end)

Section3Main:CreateButton("No Water + Lava + Acid", function ()
 
    game.Workspace.World.Ocean.Water:Destroy()
    game.Workspace.World.Regions.Gameplay.Damage.Lava:Destroy()
    game.Workspace.World.Regions.Gameplay.Damage.Acid:Destroy()
    game.Workspace.World.Regions.Gameplay.Damage:Destroy()
 
    game.Players.LocalPlayer.PlayerGui.WaterEffect:Destroy()
 
 end)

 Section3Main:CreateButton("No Explodable Walls", function ()
    game.Workspace.World.Regions.Gameplay.Explodables:Destroy()
 end)

local TeleportM = windowz:CreatePage("Teleports")

local Section1TP = TeleportM:CreateSection("Stores")

Section1TP:CreateButton("Oak Depot", function ()

   local teleport_table = {
      location1 = Vector3.new(165, 3, 153)
  }
  
  local tween_s = game:GetService('TweenService')
  local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear)
  
  local lp = game.Players.LocalPlayer
  
  function bypass_teleport(v)
      if lp.Character and 
      lp.Character:FindFirstChild('HumanoidRootPart') then
          local cf = CFrame.new(v)
          local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
          a:Play()
      end
  end
  
  bypass_teleport(teleport_table.location1)

end)

