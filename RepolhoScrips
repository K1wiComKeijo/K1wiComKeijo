local FirefliesW2 = FirefliesW:CreateSection("Color Fireflies -- Loop TP")

FirefliesW2:CreateToggle("Yellow Fireflies -- Really Common", {Toggled=false , Description = false}, function(YELLOW)
    shared["YELLOW"] = YELLOW
    while shared["YELLOW"] do
      task.wait (0.01)
      if not shared["YELLOW"] then
          break
      end
      for i,v in pairs(workspace.World.MiscPrefabs:GetDescendants()) do
        if v:IsA("Part") and v.Name == "Yellow" then
            local player = game.Players.LocalPlayer.Character
            local part = v
            player.HumanoidRootPart.CFrame = part.CFrame
        end
    end
   end
 end)

