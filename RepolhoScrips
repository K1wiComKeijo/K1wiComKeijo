local Marking1 = windowz:CreatePage("Marking")

local Marking1S = Marking1:CreateSection("Marking")

Marking1S:CreateButton("Set A Mark -- ONLY 1 (deletes previous)", function ()

for i,v in pairs(workspace:GetDescendants()) do
    if v:IsA("Part") and v.Name == "MARKING" then
       v:Destroy()
    else
        
    end
end

    local MARKINGPT = game.Players.LocalPlayer.Name
    local CFMARKING = game.workspace[MARKINGPT].HumanoidRootPart.CFrame
    local part = Instance.new("Part")
    part.Name = "MARKING"
    part.Anchored = true
    part.CanCollide = false
    part.Shape = Enum.PartType.Block
    part.Color = Color3.new(1, 0, 0)
    part.Parent = workspace
    part.CFrame = CFMARKING

    local light = Instance.new("PointLight")
    light.Name = "PointLight"
    light.Parent = workspace.MARKING
    light.Brightness = 2.5
    light.Range = 25
    
end)

Marking1S:CreateButton("Delete Mark", function ()

    for i,v in pairs(workspace:GetDescendants()) do
        if v:IsA("Part") and v.Name == "MARKING" then
           v:Destroy()
        else
            
        end
    end
    
end)

Marking1S:CreateButton("Teleport To Mark", function ()

    local player = game.Players.LocalPlayer.Character
    local part = game.Workspace.MARKING
    player.HumanoidRootPart.CFrame = part.CFrame
    
end)
